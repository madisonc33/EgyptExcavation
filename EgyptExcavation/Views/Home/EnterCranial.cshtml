@model Cranial

<div class="text-center">
    <h1 class="display-4">Enter Cranial Information</h1>
</div>

<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Colorlib Templates">
    <meta name="author" content="Colorlib">
    <meta name="keywords" content="Colorlib Templates">

    <!-- Title Page-->
    <title></title>

    <!-- Icons font CSS-->
    <link href="~/lib/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
    <link href="~/lib/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <!-- Font special for pages-->
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Vendor CSS-->
    <link href="~/lib/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/lib/vendor/datepicker/daterangepicker.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="~/css/main.css" rel="stylesheet" media="all">
</head>

<body>
    <div class="card card-4">
        <div class="card-body">
            
            <form asp-action="EnterCranial" method="post">
                <div asp-validation-summary="All"></div>
                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="MaxCranialLength">Maximum Cranial Length:</label>
                            <input asp-for="MaxCranialLength" class="input--style-4" />
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="SkullTrauma">Skull Trauma:</label>
                            <select asp-for="SkullTrauma">
                                <option value="True">True</option>
                                <option value="False">False</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="MaxCranialBreadth">Maximum Cranial Breadth:</label>
                            <input asp-for="MaxCranialBreadth" class="input--style-4" />
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="BasionBergmaHeight">Basion Bergma Height:</label>
                            <input asp-for="BasionBergmaHeight" class="input--style-4" />

                        </div>
                    </div>
                </div>
                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="BasionNasion">Basion Nasion:</label>
                            <input asp-for="BasionNasion" class="input--style-4" />
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="BasionProsithanLength">Basian Prosithan Length:</label>
                            <input asp-for="BasionProsithanLength" class="input--style-4" />
                        </div>
                    </div>
                </div>
                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="BizgoymaticDiameter">Bizgoymatic Diameter:</label>
                            <input asp-for="BizgoymaticDiameter" class="input--style-4" />
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="NasionProsthion">Nasion Prosthion:</label>
                            <input asp-for="NasionProsthion" class="input--style-4" />
                        </div>
                    </div>
                </div>
                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="MaxNasalBreadth">Maximum Nasal Breath:</label>
                            <input asp-for="MaxNasalBreadth" class="input--style-4" />
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="InterorbitalBreadth">Interorbital Breadth:</label>
                            <input asp-for="InterorbitalBreadth" class="input--style-4" />
                        </div>
                    </div>
                </div>
                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="YearOnSkull">Year On Skull:</label>
                            <input asp-for="YearOnSkull" class="input--style-4" value="0" />
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="MonthOnSkull">Month On Skull:</label>
                            <select asp-for="MonthOnSkull">
                                <option value="Jan">January</option>
                                <option value="Feb">February</option>
                                <option value="Mar">March</option>
                                <option value="Apr">April</option>
                                <option value="May">May</option>
                                <option value="Jun">June</option>
                                <option value="Jul">July</option>
                                <option value="Aug">August</option>
                                <option value="Sep">September</option>
                                <option value="Oct">October</option>
                                <option value="Nov">November</option>
                                <option value="Dec">December</option>
                            </select>
                           
                        </div>
                    </div>
                </div>
                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="SkullAtMagazine">Skull At Magazine:</label>
                            <select asp-for="SkullAtMagazine">
                                <option value="True">True</option>
                                <option value="False">False</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="col-4">
                    <div class="input-group">
                        <label asp-for="CribraOrbitalia">Cribra Orbitalia:</label>
                        <select asp-for="CribraOrbitalia">
                            <option value="True">True</option>
                            <option value="False">False</option>
                        </select>
                        <input asp-for="CribraOrbitalia" class="input--style-4" />
                    </div>
                </div>

                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="PoroticHyperostosis">Porotic Hyperostosis:</label>
                            <select asp-for="PoroticHyperostosis">
                                <option value="True">True</option>
                                <option value="False">False</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="PoroticHyperostosisLoc">Porotic Hyperostosis Location:</label>
                            <input asp-for="PoroticHyperostosisLoc" class="input--style-4" />
                        </div>
                    </div>
                </div>
                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="MetopicSuture">Metopic Suture:</label>
                            <select asp-for="MetopicSuture">
                                <option value="True">True</option>
                                <option value="False">False</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="ButtonOsteoma">Button Osteoma:</label>
                            <select asp-for="ButtonOsteoma">
                                <option value="True">True</option>
                                <option value="False">False</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="OsteologyUnknownComment">Osteology Unknown Comments: </label>
                            <input asp-for="OsteologyUnknownComment" class="input--style-4" maxlength="2" />
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="Tmjoa">TMJOA: (what is real name?)</label>
                            <select asp-for="Tmjoa">
                                <option value="True">True</option>
                                <option value="False">False</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="DateOnSkull">Date on Skull:</label>
                            <input asp-for="DateOnSkull" class="input--style-4" value="0" />
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="CranialSuture">Cranial Suture? </label>
                            <input asp-for="CranialSuture" class="input--style-4" />
                        </div>
                    </div>
                </div>
                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <label asp-for="GefunctionTotal">GE Function Total:</label>
                            <input asp-for="GefunctionTotal" class="input--style-4" />
                        </div>
                    </div>
                </div>
                <div class="p-t-15">
                    <button class="btn btn--radius-2 btn--blue" type="submit">Submit</button>
                </div>
            </form>
        </div>
    </div>

    <!-- Jquery JS-->
    <script src="~/lib/vendor/jquery/jquery.min.js"></script>
    <!-- Vendor JS-->
    <script src="~/lib/vendor/select2/select2.min.js"></script>
    <script src="~/lib/vendor/datepicker/moment.min.js"></script>
    <script src="~/lib/vendor/datepicker/daterangepicker.js"></script>

    <!-- Main JS-->
    <script src="~/js/global.js"></script>

</body>

</html>
<!-- end document-->

